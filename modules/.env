# URL cho Qdrant server (bắt buộc)
QDRANT_URL=http://localhost:6333

# Nếu dùng API Key cho Qdrant SaaS, gõ vào đây. Nếu không thì để trống hoặc xóa.
# Comment dòng này nếu không cần.
# QDRANT_API_KEY=your_api_key_here

# Tên collection mặc định (phải khớp phần client.create_collection)
COLLECTION_NAME=hybird_rerank

# Dimensional của vector dense, nếu embed tự động thì có thể bỏ hoặc ghi đúng
EMBEDDING_DIMENSION=384

# Khoảng cách metric (COSINE, EUCLIDEAN, DOT)
DISTANCE_METRIC=COSINE

# Metadata chung (dùng nếu bạn định inject metadata dạng JSON)
# Format: {"key1":"value1","key2":2}
# Nếu không dùng hãy để trống hoặc xóa
# META_EXP={"source":"startup_demo"}

# Tên vector trong Qdrant (dense + sparse)
DENSE_VECTOR_NAME=dense
SPARSE_VECTOR_NAME=sparse

# Model names cho FastEmbed
DENSE_MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2
SPARSE_MODEL_NAME=prithivida/Splade_PP_en_v1
LATE_MODEL_NAME=colbert-ir/colbertv2.0
# Path tới file payload (sample data)
PAYLOAD_PATH=./data/startups_demo.json
EMBED_MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2